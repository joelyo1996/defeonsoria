script/source = "extends Node2D
var a = 0
var entrada_mapa
func _ready():
	global.entrada_mapa = 7
	if (global.entrada_mapa == 0):
		$AnimationPlayer.play(\"texto1\")
		
	if (global.entrada_mapa == 1):
		$AnimationPlayer.play(\"texto2\")
	if (global.entrada_mapa == 7):
		$AnimationPlayer.play(\"texto5\")
	pass


func _on_botondialogo_button_up():
	
	if (global.entrada_mapa == 0):
		$AnimationPlayer.play(\"cuadro_salir\")
		$botondialogo.queue_free()
		$AnimationExclamacion.play(\"exclamacion\")
	
	if (global.entrada_mapa == 1):
		yield(get_tree().create_timer(2),\"timeout\")
		$AnimationPlayer.play(\"cuadro_salir2\")
		$AnimationExclamacion.play(\"exclamacio3\")
		yield(get_tree().create_timer(2),\"timeout\")
		$AnimationExclamacion.play(\"exclamacion\")
		
	if (global.entrada_mapa == 3):
		$AnimationPlayer.play(\"CuadroSalir3\")
		yield(get_tree().create_timer(3),\"timeout\")
		global.entrada_mapa = 4
	if (global.entrada_mapa == 5):
		$AnimationPlayer.play(\"cuadroSalir4\")
		yield(get_tree().create_timer(3),\"timeout\")
		global.entrada_mapa = 6
	if (global.entrada_mapa == 7):
		$AnimationPlayer.play(\"Cuadro_salir5\")
		yield(get_tree().create_timer(3),\"timeout\")
		global.entrada_mapa = 8
	if (global.entrada_mapa == 9):
		$AnimationPlayer.play(\"cuadro_salir6\")
		global.entrada_mapa = 10
	if (global.entrada_mapa == 11):
		$AnimationPlayer.play(\"cuadro_salir7\")
		yield(get_tree().create_timer(3),\"timeout\")
		global.entrada_mapa = 12
	pass # Replace with function body.


func _on_Area2D_body_entered(Body):
	
	if (global.entrada_mapa == 0):
		get_tree().change_scene(\"res://primer_escena_el_conedor.tscn\")
	if (global.entrada_mapa == 1):
		$AnimationPlayer.play(\"texto3\")
		yield(get_tree().create_timer(2),\"timeout\")
		global.entrada_mapa = 3
	if (global.entrada_mapa == 4):
		$AnimationPlayer.play(\"texto4\")
		yield(get_tree().create_timer(3),\"timeout\")
		global.entrada_mapa = 5
	if (global.entrada_mapa == 6):
		$AnimationPlayer.play(\"Salir de escena\")
		yield(get_tree().create_timer(3),\"timeout\")
		get_tree().change_scene(\"res://escenas/Ferreteria.tscn\")
	if (global.entrada_mapa == 8):
		$AnimationPlayer.play(\"texto6\")
		yield(get_tree().create_timer(3),\"timeout\")
		global.entrada_mapa = 9
	if (global.entrada_mapa == 10):
		$AnimationPlayer.play(\"texto7\")
		yield(get_tree().create_timer(4),\"timeout\")
		global.entrada_mapa = 11
	if (global.entrada_mapa == 12):
		$AnimationPlayer.play(\"Salir de escena\")
		yield(get_tree().create_timer(4),\"timeout\")
		get_tree().change_scene(\"res://escenas/escuela.tscn\")
	pass # Replace with function body.
func _on_AnimationPlayer_animation_finished(anim_name):
	if anim_name == \"texto3\":
		global.entrada_mapa = global.entrada_mapa + 1
"

