script/source = "extends Node2D

func _ready():
	global.entrada_mapa = 1
	if (global.entrada_mapa == 0):
		$AnimationPlayer.play(\"texto1\")
		
	if (global.entrada_mapa == 1):
		$AnimationPlayer.play(\"texto2\")
	pass


func _on_botondialogo_button_up():
	
	if (global.entrada_mapa == 0):
		$AnimationPlayer.play(\"cuadro_salir\")
		$botondialogo.queue_free()
		$AnimationExclamacion.play(\"exclamacion\")
	
	if (global.entrada_mapa == 1):
		$AnimationPlayer.play(\"cuadro_salir2\")
		$AnimationExclamacion.play(\"exclamacio3\")
		yield(get_tree().create_timer(2),\"timeout\")
		$AnimationExclamacion.play(\"exclamacion\")
	pass # Replace with function body.


func _on_Area2D_body_entered(Body):
	if (global.entrada_mapa == 0):
		get_tree().change_scene(\"res://primer_escena_el_conedor.tscn\")
	if (global.entrada_mapa == 1):
		$AnimationPlayer.play(\"texto3\")
		$KinematicBody2D/CollisionShape2D.queue_free()
		
	pass # Replace with function body.
"

